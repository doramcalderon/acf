<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>


<GridPane vgap="5.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="es.uma.pfc.is.bench.benchmarks.newbm.NewBenchFormController">
  <columnConstraints>
    <ColumnConstraints hgrow="SOMETIMES" />
  </columnConstraints>
  <rowConstraints>
      <RowConstraints fillHeight="false" vgrow="SOMETIMES" />
    <RowConstraints fillHeight="false" vgrow="SOMETIMES" />
    <RowConstraints fillHeight="false" vgrow="ALWAYS" />
    <RowConstraints vgrow="SOMETIMES" />
      <RowConstraints fillHeight="false" vgrow="SOMETIMES" />
  </rowConstraints>
   <children>
       <HBox fillHeight="false" spacing="5.0" GridPane.columnSpan="2">
         <children>
            <Button fx:id="btnSave" mnemonicParsing="false" onAction="#handleSaveButton" prefHeight="47.0" prefWidth="55.0" text="%btn.save" />
            <Button fx:id="btnClear" mnemonicParsing="false" onAction="#handleClearButton" prefHeight="47.0" prefWidth="55.0" text="%btn.clear" />
         </children>
      </HBox>
      <HBox spacing="5.0" GridPane.rowIndex="1">
         <children>
            <Label text="Name:" />
            
                        <TextField fx:id="txtName" GridPane.columnIndex="1" GridPane.rowIndex="1" HBox.hgrow="ALWAYS" />
         </children>
      </HBox>
      
                  <ListView fx:id="algorithmsList" GridPane.columnSpan="2" GridPane.rowIndex="3" />
      <Label text="%algorithms" GridPane.columnSpan="2" GridPane.rowIndex="2" />
      
                  <Label fx:id="lbErrorMessages" minWidth="5.0" textFill="#f22004" GridPane.columnSpan="2" GridPane.rowIndex="4" />
   </children>
   <padding>
      <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
   </padding>
</GridPane>
