<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.chart.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane id="AnchorPane" minWidth="800.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="es.uma.pfc.is.bench.algorithms.results.ResultsController">
   <children>
      <VBox layoutY="97.0" prefHeight="969.0" prefWidth="992.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0" GridPane.rowIndex="1">
         <children>
            <TitledPane fx:id="resultsTitledPane" alignment="TOP_LEFT" text="%title.results">
               <content>
                  <SplitPane dividerPositions="0.5">
                     <items>
                        <TreeTableView fx:id="tableResults">
                          <columns>
                            <TreeTableColumn fx:id="nameColumn" editable="false" minWidth="200.0" prefWidth="-1.0" text="%column.results.name" />
                              <TreeTableColumn fx:id="timeColumn" editable="false" minWidth="80.0" prefWidth="-1.0" style="-fx-text-alignment: RIGHT;" text="%column.results.time" />
                              <TreeTableColumn fx:id="inputColumn" editable="false" minWidth="150.0" prefWidth="-1.0" text="%column.results.input" />
                              <TreeTableColumn fx:id="outputColumn" editable="false" minWidth="150.0" prefWidth="-1.0" text="%column.results.output" />
                          </columns>
                        </TreeTableView>
                        <AnchorPane prefHeight="200.0" prefWidth="200.0" GridPane.columnIndex="1">
                                   <fx:include source="/es/uma/pfc/is/bench/benchmarks/results/ResultsViewer.fxml"/>
                              </AnchorPane>
                     </items>
                  </SplitPane>
               </content>
            </TitledPane>
            <TitledPane fx:id="statsTiltedPane" text="%title.statistics">
               <content>
                  <TableView fx:id="tableStatistics" tableMenuButtonVisible="true" GridPane.columnSpan="2" GridPane.rowIndex="3">
                    <columns>
                      <TableColumn prefWidth="75.0" text="C1" />
                      <TableColumn prefWidth="75.0" text="C2" />
                    </columns>
                  </TableView>
               </content>
            </TitledPane>
         </children>
      </VBox>
   </children>
</AnchorPane>
