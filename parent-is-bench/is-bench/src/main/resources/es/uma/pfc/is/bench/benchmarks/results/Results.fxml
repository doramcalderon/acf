<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.chart.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane xmlns:fx="http://javafx.com/fxml/1" id="AnchorPane" minWidth="800.0" xmlns="http://javafx.com/javafx/8" fx:controller="es.uma.pfc.is.bench.algorithms.results.ResultsController">
   <children>
      <GridPane AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <columnConstraints>
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" />
         </columnConstraints>
         <rowConstraints>
            <RowConstraints maxHeight="100.0" minHeight="10.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
         </rowConstraints>
         <children>
            <VBox GridPane.rowIndex="1">
               <children>
                  <TitledPane fx:id="resultsTitledPane" alignment="TOP_LEFT" text="%title.results">
                     <content>
                        <GridPane hgap="5.0">
                           <columnConstraints>
                              <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" percentWidth="60.0" />
                              <ColumnConstraints hgrow="SOMETIMES" minWidth="0.0" percentWidth="40.0" />
                           </columnConstraints>
                           <rowConstraints>
                              <RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
                           </rowConstraints>
                           <children>
                              <TreeTableView fx:id="tableResults">
                                <columns>
                                  <TreeTableColumn fx:id="nameColumn" editable="false" minWidth="200.0" prefWidth="-1.0" text="Name" />
                                    <TreeTableColumn fx:id="timeColumn" editable="false" minWidth="80.0" prefWidth="-1.0" style="-fx-text-alignment: RIGHT;" text="Time (ms)" />
                                    <TreeTableColumn fx:id="inputColumn" editable="false" minWidth="150.0" prefWidth="-1.0" text="Input" />
                                    <TreeTableColumn fx:id="outputColumn" editable="false" minWidth="150.0" prefWidth="-1.0" text="Output" />
                                </columns>
                              </TreeTableView>
                              <VBox prefHeight="200.0" prefWidth="100.0" spacing="5.0" GridPane.columnIndex="1">
                                 <children>
                                    <fx:include source="/es/uma/pfc/is/bench/benchmarks/results/ResultsViewer.fxml"/>
                                 </children>
                              </VBox>
                           </children>
                        </GridPane>
                     </content>
                  </TitledPane>
                  <TitledPane fx:id="statsTiltedPane" text="%title.statistics">
                     <content>
                        <TableView prefHeight="200.0" prefWidth="200.0" GridPane.columnSpan="2" GridPane.rowIndex="3" fx:id="tableStatistics">
                          <columns>
                            <TableColumn prefWidth="75.0" text="C1" />
                            <TableColumn prefWidth="75.0" text="C2" />
                          </columns>
                        </TableView>
                     </content>
                  </TitledPane>
               </children>
            </VBox>
            <ToolBar layoutX="5.0" layoutY="5.0" maxHeight="90.0">
              <items>
                <Button mnemonicParsing="false" prefHeight="80.0" prefWidth="80.0" text="Open" />
                  <Button layoutX="10.0" layoutY="10.0" mnemonicParsing="false" prefHeight="80.0" prefWidth="80.0" text="Save" />
                  <Button layoutX="10.0" layoutY="10.0" mnemonicParsing="false" prefHeight="80.0" prefWidth="80.0" text="Delete" />
              </items>
            </ToolBar>
         </children>
      </GridPane>
   </children>
</AnchorPane>
