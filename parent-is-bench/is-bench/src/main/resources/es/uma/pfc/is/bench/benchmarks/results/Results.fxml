<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.chart.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="es.uma.pfc.is.bench.algorithms.results.ResultsController">
   <children>
      <GridPane hgap="5.0" layoutX="10.0" layoutY="10.0" prefHeight="820.0" prefWidth="600.0" vgap="5.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <columnConstraints>
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
         </columnConstraints>
         <padding>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </padding>
         <rowConstraints>
            <RowConstraints maxHeight="90.0" minHeight="10.0" prefHeight="90.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" percentHeight="25.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" percentHeight="25.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" percentHeight="25.0" vgrow="SOMETIMES" />
         </rowConstraints>
         <children>
            <VBox GridPane.rowIndex="2">
               <children>
                  <Label text="%input">
                     <font>
                        <Font size="14.0" />
                     </font>
                  </Label>
                  <TextArea fx:id="txtInput" prefHeight="200.0" prefWidth="200.0" GridPane.rowIndex="2" />
               </children>
            </VBox>
            <VBox GridPane.columnIndex="1" GridPane.rowIndex="2">
               <children>
                  <Label text="%output" />
                  <TextArea fx:id="txtOutput" prefHeight="200.0" prefWidth="200.0" GridPane.columnIndex="1" GridPane.rowIndex="2" />
               </children>
            </VBox>
            <TableView prefHeight="200.0" prefWidth="200.0" GridPane.columnSpan="2" GridPane.rowIndex="3">
              <columns>
                <TableColumn prefWidth="75.0" text="C1" />
                <TableColumn prefWidth="75.0" text="C2" />
              </columns>
            </TableView>
            <TreeTableView fx:id="tableResults" GridPane.columnSpan="2" GridPane.rowIndex="1">
              <columns>
                <TreeTableColumn fx:id="nameColumn" editable="false" prefWidth="116.0" text="Name" />
                <TreeTableColumn fx:id="dateColumn" editable="false" prefWidth="126.0" text="Date" />
                  <TreeTableColumn fx:id="timeColumn" editable="false" prefWidth="62.0" text="Time (ms)" />
                  <TreeTableColumn fx:id="inputColumn" editable="false" prefWidth="174.0" text="Input" />
                  <TreeTableColumn fx:id="outputColumn" editable="false" minWidth="0.0" prefWidth="164.0" text="Output" />
              </columns>
            </TreeTableView>
            <ToolBar GridPane.columnSpan="2">
              <items>
                <Button mnemonicParsing="false" prefHeight="80.0" prefWidth="80.0" text="Open" />
                  <Button layoutX="10.0" layoutY="10.0" mnemonicParsing="false" prefHeight="80.0" prefWidth="80.0" text="Save" />
                  <Button layoutX="10.0" layoutY="10.0" mnemonicParsing="false" prefHeight="80.0" prefWidth="80.0" text="Delete" />
              </items>
            </ToolBar>
         </children>
      </GridPane>
   </children>
</AnchorPane>
